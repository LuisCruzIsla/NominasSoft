<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Capa1_Presentacion.Controlador.FormProcesarPagoController">
    <stylesheets>
        <URL value="@/Capa1_Presentacion/Estilos/formprocesarpago.css" />
    </stylesheets>
   <children>
      <StackPane fx:id="stackPane" prefHeight="600.0" prefWidth="600.0">
         <children>
            <AnchorPane prefHeight="600.0" prefWidth="600.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="600.0" prefWidth="600.0">
                     <children>
                        <VBox alignment="CENTER" prefWidth="500.0">
                           <children>
                              <HBox alignment="CENTER" spacing="16.0">
                                 <children>
                                    <DatePicker fx:id="dpFechaInicio"  editable="false" promptText="Fecha Inicio"/>
                                    <DatePicker fx:id="dpFechaFin"  editable="false" promptText="Fecha Fin"/>
                                    <JFXButton fx:id="btnProcesar" buttonType="RAISED" style="-fx-background-color: #00BCD4; -fx-graphic: url(&quot;Imagenes/search.png&quot;);" text="Procesar" textFill="WHITE"  onAction="#procesar">
                                      <font>
                                         <Font size="14.0" />
                                      </font>
                                   </JFXButton>
                                 </children>
                              </HBox>
                              <JFXTreeTableView fx:id="treeView">
                              </JFXTreeTableView>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </VBox>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
